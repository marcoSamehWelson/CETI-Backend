                    </th>

                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => Model.FromDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.ToDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.TotalBeneficiaries)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.FullAge)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.InSchool)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.TotalIncome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => Model.BranchName)
                    </td>


                </tr>
            </tbody>
        </table>
    </div>

